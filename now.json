{
	"version": 2,
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		}
	],
	"env": {
		"APP_AUTH": "@app_auth",
		"DATABASE_URL": "@database_url"
	},
	"build": {
		"env": {
			"APP_AUTH": "@app_auth",
			"DATABASE_URL": "@database_url",
			"DEBUG": "prisma-client,engine"
		}
	}
}
